  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "react/prop-types": "off"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "module": true,
    "__dirname": true
  }
}
